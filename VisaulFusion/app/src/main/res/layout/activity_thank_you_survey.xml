<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/install_set_appointment_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dsouchon.wayo.visualfusion.ThankYouSurvey">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="40dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="105dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Survey Complete. Good Job!"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="40dp"
                tools:layout_editor_absoluteY="28dp" />
            <TextView
                android:id="@+id/textViewThankYou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Thank the outlet owner for their time and state that one member of the Project management team or his local representative from EABL will be in contact shortly to coordinate the installation process and date."
                android:textAlignment="center"
                android:textSize="14sp" />

            <Button
                android:id="@+id/button9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="clickProceedToInstallHome"
                android:text="OK" />
        </LinearLayout>



</LinearLayout>
